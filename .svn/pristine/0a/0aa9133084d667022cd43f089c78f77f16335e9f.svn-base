package com.zkxltech.marksdk.`interface`

/**
 *创建作者       : albus
 *创建时间       : 2019/11/30
 *Fuction(类描述):通信接口回调
 */
interface NFCCommunicationInterface {
    //写入数据成功
    fun NFCCommunicationSucess()
    //写入数据失败
    fun NFCCommunicationFail(message:String)
    //写入数据之前的操作
    fun NFCCommunicationAgo()
    //写入进度的回调
    fun NFCCommunicationProgress(progress:Int)
    //写入数据成功后返回的结果
    fun NFCCommunicationSucessResult(message:String)
}